{
  "expo": {
    "name": "Arena Pro",
    "slug": "arena-pro",
    "scheme": "arenapro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/images/arena_pro_logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/images/arena_pro_logoqr.png",
      "resizeMode": "contain",
      "backgroundColor": "#004d43"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "arenapropk.online",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIViewControllerBasedStatusBarAppearance": true,
        "UIStatusBarStyle": "UIStatusBarStyleDarkContent"
      },
      "runtimeVersion": {
        "policy": "appVersion"
      },
      "statusBar": {
        "barStyle": "dark-content",
        "hidden": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/images/arena_pro_logoqr.png",
        "backgroundColor": "#004d43"
      },
      "package": "arenapropk.online",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.RECORD_AUDIO"
      ],
      "runtimeVersion": "1.0.0",
      "statusBar": {
        "barStyle": "dark-content",
        "backgroundColor": "#FFFFFF",
        "translucent": true
      },
      "navigationBar": {
        "backgroundColor": "#FFFFFF",
        "barStyle": "dark-content"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-location",
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you set your profile picture.",
          "cameraPermission": "The app accesses your camera to let you take a profile picture."
        }
      ]
    ],
    "newArchEnabled": false,
    "extra": {
      "eas": {
        "projectId": "3e7192c7-524e-4146-801c-3960363da458"
      }
    }
  }
}